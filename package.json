  "main": "./src/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "liqiang",
      "properties": {
        "liqiang.jsonName": {
          "description": "JSON:文件名称",
          "type": "string",
          "default": "__db__"
        },
        "liqiang.jsonIgnore": {
          "description": "JSON:忽略文件夹",
          "type": "string",
          "default": "(src|dist|images?|style|script)"
        }
      }
    },
    "commands": [
      {
        "command": "liqiang.removeBlank",
        "title": "删除剪贴板字符空格"
      },
      {
        "command": "liqiang.fileFactory",
        "title": "加工文件"
      },
      {
        "command": "liqiang.createJson",
        "title": "生成JSON文件"
      },
      {
        "command": "liqiang.createWXMFiles",
        "title": "创建小程序文件"
      },
      {
        "command": "liqiang.createIndexFiles",
        "title": "创建Index文件"
      }
    ],
    "keybindings": [
      {
        "command": "liqiang.removeBlank",
        "key": "ctrl+y ctrl+b"
      },
      {
        "command": "liqiang.fileFactory",
        "key": "ctrl+y ctrl+c"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "liqiang.fileFactory"
        }
      ],
      "explorer/context": [
        {
          "command": "liqiang.fileFactory",
          "group": "liqiang-@2"
        },
        {
          "command": "liqiang.createJson",
          "group": "liqiang-@3"
        },
        {
          "command": "liqiang.createIndexFiles",
          "group": "liqiang-@4"
        },
        {
          "command": "liqiang.createWXMFiles",
          "group": "liqiang-@5"
        }
      ]
    },
    "snippets": []
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "copy-paste": "^1.3.0"
  }
}
