{
  "name": "liqiang-vsutils",
  "displayName": "liqiang-vsutils",
  "description": "TOOL FOR SELF",
  "version": "1.1.0",
  "publisher": "liqiang0335",
  "author": "liqiang0335 <liqiang0335@gmail.com>",
  "bugs": {
    "email": "liqiang0335@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liqiang0335/liqiang-vsutils.git"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "keywords": [
    "liqiang"
  ],
  "activationEvents": [
    "onCommand:liqiang.SaveImage",
    "onCommand:liqiang.ReplaceModule"
  ],
  "main": "./src/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "liqiang",
      "properties": {
        "liqiang.saveURL": {
          "description": "图片上传地址",
          "type": "string",
          "default": ""
        },
        "liqiang.command": {
          "description": "CMD用户命令",
          "type": "string",
          "default": "common"
        },
        "liqiang.runType": {
          "description": "CMD运行方式",
          "type": "string",
          "enum": [
            "exec",
            "terminal"
          ],
          "default": "exec"
        }
      }
    },
    "commands": [
      {
        "command": "liqiang.removeBlank",
        "title": "删除剪贴板字符空格"
      },
      {
        "command": "liqiang.CreateBookJSON",
        "title": "🚩 生成JSON文件"
      },
      {
        "command": "liqiang.MarkdownToHTML",
        "title": "🚩 MD转换HTML"
      },
      {
        "command": "liqiang.UserDefined",
        "title": "🚩 文件处理(用户设置的类型)"
      },
      {
        "command": "liqiang.CopyPathMd5",
        "title": "🚩 复制路径MD5"
      },
      {
        "command": "liqiang.SplitBookJSON",
        "title": "🚩 拆分JSON数据"
      },
      {
        "command": "liqiang.CopyRelativePages",
        "title": "🚩 复制相对Pages路径"
      }
    ],
    "keybindings": [
      {
        "command": "liqiang.ReplaceModule",
        "key": "cmd+'"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "liqiang.UserDefined"
        }
      ],
      "explorer/context": [
        {
          "command": "liqiang.CreateBookJSON",
          "group": "liqiang-@01"
        },
        {
          "command": "liqiang.SplitBookJSON",
          "group": "liqiang-@02"
        },
        {
          "command": "liqiang.CopyPathMd5",
          "group": "liqiang-@03"
        },
        {
          "command": "liqiang.MarkdownToHTML",
          "group": "liqiang-@04"
        },
        {
          "command": "liqiang.UserDefined",
          "group": "liqiang-@05"
        },
        {
          "command": "liqiang.CopyRelativePages",
          "group": "liqiang-@06"
        }
      ]
    },
    "snippets": []
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "eslint": "^4.11.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "copy-paste": "^1.3.0",
    "form-data": "^4.0.0"
  }
}